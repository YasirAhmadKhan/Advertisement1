<% if flash[:notice] %>
  <%= content_tag :h1, flash[:notice] %>
<% else %>
  <%= content_tag :h1, "Please Log in" %>
<% end %>

<%=form_with url: session_path([:id => nil]) do|f| %>
<%= f.text_field :email%><br>
<%= f.password_field :password %><br>
<%= link_to "Sign Up", new_user_path %>
<%= link_to (f.submit "Sign In!"), book_index_path %>
<% end %>